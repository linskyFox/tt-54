# addthis id
addthis.pubid="<%=@addthis_pubid%>"

# ignore SSL error
ws.ssl.loose.disableHostnameVerification=true

# post body size
parsers.text.maxLength=10M

# db reset allowed?
liquibase.reset.allowed="<%=@db_reset_allowed%>"

# Use order processing
order.processing="<%=@order_step%>"

play {
  akka {
    akka.loggers = ["akka.event.slf4j.Slf4jLogger"]
    loglevel = WARNING
    actor {
      default-dispatcher = {
        fork-join-executor {
          parallelism-min = 300
          parallelism-max = 300
        }
      }
    }
  }
}
