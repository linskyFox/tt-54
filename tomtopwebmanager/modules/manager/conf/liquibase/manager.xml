	<changeSet author="reason" id="t_admin_user-create">
		<createTable tableName="t_admin_user" remarks="后台用户表">
			<column name="iid" type="INTEGER" autoIncrement="true"
				remarks="主键">
				<constraints nullable="false" primaryKey="true" />
			</column>
			<column name="cusername" type="VARCHAR(20)" remarks="姓名" />
			<column name="cjobnumber" type="VARCHAR(12)" remarks="工号" />
			<column name="cpasswd" type="VARCHAR(50)" remarks="密码" />
			<column name="cemail" type="VARCHAR(50)" remarks="邮箱" />
			<column name="cphone" type="VARCHAR(50)" remarks="联系电话" />
			<column name="bwebsiteadmin" type="BOOL" defaultValue="FALSE"
				remarks="是否平台管理员" />
			<column name="badmin" type="BOOL" defaultValue="FALSE"
				remarks="是否超级管理员" />
			<column name="ccreateuser" type="VARCHAR(20)" remarks="创建人" />
			<column name="dcreatedate" type="DATETIME" defaultValue="now()"
				remarks="创建时间" />
		</createTable>
	</changeSet>


